<?xml version="1.0" encoding="utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/swipe_refresh_layout_recycler_view_detail_monitoring"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="id.co.perhutani.sisdhbukuobor.FragmentUi.MonitoringKlsHtnPenampakan.DetailMonitoringFragment"
    tools:showIn="@layout/fragment_detail_monitoring">

    <LinearLayout
        android:layout_marginTop="45dp"
        android:background="@drawable/ic_background_list_home"
        android:layout_marginBottom="5dp"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:orientation="vertical">

        <androidx.recyclerview.widget.RecyclerView
            android:clickable="true"
            android:scrollbars="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/detail_monitoring_recyclerview">
        </androidx.recyclerview.widget.RecyclerView>

    </LinearLayout>

</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>


